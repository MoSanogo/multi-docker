{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions":[

    {
        "name":"client",
        "image":"msanogo/multi-client",
        "hostname":"client",
        "essential":false,
        "memory":128
    },
    {
        "name":"server",
        "image":"msanogo/multi-server",
        "hostname":"api",
        "essential":false,
        "memory":128
    },
    {
         "name":"worker",
         "image":"msanogo/multi-worker",
         "hostname":"worker",
         "essential":false,
         "memory":128
    },
    {
        "name":"nginx",
        "image":"msanogo/multi-nginx",
        "essential":true,
        "memory":128,
        "portMappings":[
            {
                "hostPort":80,
                "containerPort":80
            }
        ],
        "links":[
            "client","server"
        ]
    }
    ]
}